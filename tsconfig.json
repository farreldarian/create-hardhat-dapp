{
  "compilerOptions": {
    "target": "es2019",
    "moduleResolution": "node",
    "lib": ["ESNext"],
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": false,
    "outDir": "dist",
    "noImplicitAny": false
  },
  "exclude": ["templates", "dist"]
}
